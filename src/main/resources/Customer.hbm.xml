<hibernate-mapping>
  <class name="com.mycompany.finalproject.pojo.Customer" table="customer">
    <id name="id" column="id" type="int">
      <generator class="native"/>
    </id>
    <property name="username" column="username" type="string"/>
    <property name="firstName" column="fname" type="string"/>
    <property name="lastName" column="lname" type="string"/>
    <property name="age" column="age" type="int"/>
    <property name="email" column="email" type="string"/>
    <property name="address" column="address" type="string"/>
    <property name="phone_number" column="phone_number" type="string"/>
    <property name="password" column="password" type="string"/>
  </class>
</hibernate-mapping>