<hibernate-mapping>
  <class name="com.mycompany.finalproject.pojo.Restaurant" table="restaurant">
    <id name="restaurant_id" column="id" type="int">
      <generator class="native"/>
    </id>
    <property name="restaurant_name" column="restaurant_name" type="string"/>
    <property name="phone_number" column="phone_number" type="string"/>
    <property name="email" column="email" type="string"/>
    <property name="address" column="address" type="string"/>
    <property name="restaurant_category" column="category" type="string"/>
    <property name="intro" column="intro" type="string"/>
    <property name="password" column="password" type="string"/>
    <property name="balance" column="balance" type="double"/>
    <property name="username" column="username" type="string"/>
  </class>
</hibernate-mapping>